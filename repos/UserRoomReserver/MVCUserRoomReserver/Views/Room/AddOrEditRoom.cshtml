@model MVCUserRoomReserver.Models.mvcRoomModel
@{
    ViewBag.Title = "Salones";
}

<h2>@ViewBag.Title</h2>


<div>
    <div class="jumbotron text-center">

        @using (Html.BeginForm())
        {

            @Html.HiddenFor(model => model.id)
            <div class="form-group col col-md-6 offset-2">
                @Html.LabelFor(model => model.roomName, new { @class = "col col-md-4" })
                @Html.TextBoxFor(model => model.roomName, new { @class = "col col-md-5" })*
            </div>
            <div class="form-group col col-md-12 text-center">
                <input type="submit" value="Guardar" class="btn btn-info col col-md-6" />
            </div>
            <div class="text-center">
                    <span class="text-center">@Html.ValidationMessageFor(model => model.roomName)</span>
            </div>
        }
    </div>
</div>


    @section scripts{
        @Scripts.Render("~/bundles/jqueryval");
    }
