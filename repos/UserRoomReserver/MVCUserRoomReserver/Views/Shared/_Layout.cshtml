<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/alertify.min.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/themes/default.min.css" rel="stylesheet" />
</head>
<body>
    <div>
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("User Rooms Reserver", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" }) |
                @Html.ActionLink("Inicio", "Index", "Home") |
                @Html.ActionLink("Usuarios", "Index", "User") |
                @Html.ActionLink("Salones", "Index", "Room") |
                @Html.ActionLink("Reservas", "Index", "spRBD") |
               
            </div>
            
        </div>
    </div>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer class="text-center">
                <p>&copy; @DateTime.Now.Year - Outsorcing</p>
                <p>Powered by CriWong</p>
            </footer>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        <script src="~/Scripts/alertify.min.js"></script>
        @RenderSection("scripts", required: false)
</body>
</html>
